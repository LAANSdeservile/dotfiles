#xdotool type --window [window ID] Hello World!/bin/sh
#aqw ability spam macro.
#!/bin/sh
sleep 3 
id=$(xdotool getactivewindow)
echo $id
while true
do
	xdotool type --window $id 5
        sleep 0.3
	xdotool type --window $id 3
	sleep 0.3
	xdotool type --window $id 2 
	sleep 0.3
	xdotool type --window $id 4 
	sleep 0.3
	xdotool type --window $id 1
	sleep 0.3
	xdotool type --window $id 2
done
